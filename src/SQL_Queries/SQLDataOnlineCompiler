CREATE TABLE EMPLOYEE (
  empId INTEGER PRIMARY KEY,
  name TEXT NOT NULL,
  dept TEXT NOT NULL,
  salary INT NOT NULL,
  manager TEXT
);

CREATE TABLE DEPARTMENT (
  deptID INTEGER PRIMARY KEY,
  deptname TEXT NOT NULL
);

